<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.unab.eli"
    version   = "1.0.0">

<name>Demo-FCM_vAl</name>

<description>
Aplicacion de Ejemplo Firebase Cloud Messaging_vAl.
</description>

<author href="http://www.unab.cl" email="eliana.alexia@gmail.com">
Eliana Urrutia
</author>

<preference name="target-device" value="universal" />
<preference name="orientation" value="portrait" />
<preference name="exit-on-suspend" value="true" />
<preference name="fullscreen" value="false" />

<icon src="icons/ios/icon.png" width="57" height="57" />
<icon src="icons/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
<icon src="icons/ios/icon_at_2x.png" width="114" height="114" />

<icon src="icons/android/ldpi.png" gap:platform="android" gap:density="ldpi" />
<icon src="icons/android/mdpi.png" gap:platform="android" gap:density="mdpi" />
<icon src="icons/android/hdpi.png" gap:platform="android" gap:density="hdpi" />
<icon src="icons/android/xhdpi.png" gap:platform="android" gap:density="xhdpi" />

<icon src="icons/winphone/icon.png" gap:platform="winphone" />
<icon src="icons/winphone/tileicon.png" gap:platform="winphone" gap:role="background" />

<gap:splash src="splashes/ios/Default.png" width="320" height="480" />
<gap:splash src="splashes/ios/Default_at_2x.png" width="640" height="960" />
<gap:splash src="splashes/ios/Default_iphone5.png" width="640" height="1136" />
<gap:splash src="splashes/ios/splash_ipad_Landscape.png" width="1024" height="768" />
<gap:splash src="splashes/ios/splash_ipad_Portrait.png" width="768" height="1024" />

<gap:splash src="splashes/android/ldpi.png" gap:platform="android" gap:density="ldpi" />
<gap:splash src="splashes/android/mdpi.png" gap:platform="android" gap:density="mdpi" />
<gap:splash src="splashes/android/hdpi.png" gap:platform="android" gap:density="hdpi" />
<gap:splash src="splashes/android/xhdpi.png" gap:platform="android" gap:density="xhdpi" />
   <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />

	<resource-file src="google-services.json" target="google-services.json" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-fcm" />
    

    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="android-minSdkVersion" value="19" />
<access origin="http://72.14.183.67/ws/*" />

</widget>